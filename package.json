{
  "name": "@carlipa/pandora-compose",
  "version": "1.0.0",
  "description": "Pandora Compose",
  "main": "lib/index.js",
  "scripts": {
    "start": "$(npm bin)/gulp serve",
    "compile": "gulp build",
    "test": "npm run test:lint && npm run test:mocha",
    "test:lint": "$(npm bin)/eslint src",
    "test:mocha": "NODE_ENV=test node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill 'src/**/__tests__/*.js' ",
    "test:coverage": "NODE_ENV=test $(npm bin)/babel-node $(npm bin)/isparta cover ./node_modules/mocha/bin/_mocha -- 'src/**/__tests__/*.js'",
    "prepublish": "npm run compile"
  },
  "engines": {
    "node": ">=4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carlipa/pandora-compose.git"
  },
  "author": {
    "name": "Guillaume Coupelant",
    "email": "gcoupelant@carlipa.com"
  },
  "license": "MIT",
  "dependencies": {
    "@carlipa/pandora-docker": "^1.0.0",
    "@reactivex/rxjs": "^5.0.0-beta.6",
    "bluebird": "^3.3.5",
    "lodash": "^4.11.1",
    "source-map-support": "^0.4.0",
    "tar-fs": "^1.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-plugin-relative-import": "^1.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "bulk": "^2.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "del": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-mgcrea": "^6.1.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "gulp-babel": "^6.1.2",
    "gulp-debug": "^2.1.2",
    "gulp-nodemon": "^2.0.6",
    "gulp-plumber": "^1.1.0",
    "gulp-pre": "^4.0.0-alpha.4",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-watch": "^4.3.5",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "stream-combiner2": "^1.1.1"
  }
}
